{
  "name": "max-medical-booking",
  "homepage": "https://ser2331.github.io/max-medical-booking",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tunnel": "vite --host 0.0.0.0",
    "dev:prod": "vite --mode production",
    "dev:full": "concurrently \"npm run dev:tunnel\" \"lt --port 5173 --subdomain my-max-app\"",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "npm run prettier && npm run lint:fix",
    "tunnel": "lt --port 5173 --subdomain my-max-app",
    "tunnel:unique": "lt --port 5173 --subdomain my-max-app-$(date +%s)",
    "dev:unique": "concurrently \"npm run dev:tunnel\" \"npm run tunnel:unique\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@maxhub/max-ui": "^0.1.12",
    "@reduxjs/toolkit": "^2.9.2",
    "@vkontakte/vk-tunnel": "^0.2.9",
    "crypto-js": "^4.2.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.2.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.35",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
